<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.xcsa.gateway.mapper.LoginRecordMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="cn.com.xcsa.gateway.domain.po.LoginRecord">
            <id column="id" property="id"/>
            <result column="user_id" property="userId"/>
            <result column="login_name" property="loginName"/>
            <result column="login_type" property="loginType"/>
            <result column="tenant_id" property="tenantId"/>
            <result column="token" property="token"/>
            <result column="status" property="status"/>
            <result column="client_version" property="clientVersion"/>
            <result column="ip_addr" property="ipAddr"/>
            <result column="system_name" property="systemName"/>
            <result column="system_version" property="systemVersion"/>
            <result column="terminal_type" property="terminalType"/>
            <result column="create_time" property="createTime"/>
            <result column="deleted" property="deleted"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, user_id, login_name, login_type, tenant_id, token, status, client_version, ip_addr, system_name, system_version, terminal_type, create_time, deleted
        </sql>

</mapper>
