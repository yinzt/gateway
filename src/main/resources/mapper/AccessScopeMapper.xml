<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.xcsa.gateway.mapper.AccessScopeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.com.xcsa.gateway.domain.po.AccessScope">
        <id column="id" property="id"/>
        <result column="visit_name" property="visitName"/>
        <result column="visit_range" property="visitRange"/>
        <result column="visit_member" property="visitMember"/>
        <result column="visit_space" property="visitSpace"/>
        <result column="create_time" property="createTime"/>
        <result column="sid" property="sid"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,visit_name,visit_range,visit_member,visit_space,create_time,sid
    </sql>

</mapper>