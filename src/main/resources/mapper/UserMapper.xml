<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.xcsa.gateway.mapper.UserMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="cn.com.xcsa.gateway.domain.po.User">
            <id column="id" property="id"/>
            <result column="name" property="name"/>
            <result column="login_name" property="loginName"/>
            <result column="password" property="password"/>
            <result column="tenant_id" property="tenantId"/>
            <result column="phone" property="phone"/>
            <result column="email" property="email"/>
            <result column="activate_status" property="activateStatus"/>
            <result column="lock_status" property="lockStatus"/>
            <result column="user_status" property="userStatus"/>
            <result column="source" property="source"/>
            <result column="create_time" property="createTime"/>
            <result column="update_time" property="updateTime"/>
            <result column="deleted" property="deleted"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, name, login_name, password, tenant_id, phone, email, activate_status, lock_status, user_status, source, create_time, update_time, deleted
        </sql>

</mapper>
