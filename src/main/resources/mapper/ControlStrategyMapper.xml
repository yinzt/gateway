<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.xcsa.gateway.mapper.ControlStrategyMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.com.xcsa.gateway.domain.po.ControlStrategy">
        <id column="id" property="id"/>
        <result column="control_name" property="controlName"/>
        <result column="control_strategy" property="controlStrategy"/>
        <result column="control_member" property="controlMember"/>
        <result column="create_time" property="createTime"/>
        <result column="cid" property="cid"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,control_name,control_strategy,control_member,create_time,cid
    </sql>

</mapper>